package com.assighnment;

public class Differenciate {
	
	    public static void main(String[] args){
	  
	        int i = 0;
	  
	        for (i = 0; i <= 5; i++) {
	        	if(i==4);
	            System.out.println("This is for loop "+i );
	        }
	    
	    
	  i=10;
	        while (i < 10);{
		        i++;
	            System.out.println("This is while loop");


	            }
	        
	        
	    }
}
	


