package com.assighnment;

public class Differenciate {
	
	    public static void main(String[] args){
	  
	        int i = 0;
	  
	        for (i = 0; i <= 5; i++) {
	        	if(i==2);
	        	break;
	           // System.out.println("This is for loop "+i );
	        }
	    
	    
	        int a=10;
	        while (a < 10);{
		        a++;
	            System.out.println("This is while loop" + a);

	            }
	        
	        
	    }
}
	


