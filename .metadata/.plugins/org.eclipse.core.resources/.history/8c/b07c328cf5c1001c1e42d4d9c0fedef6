package com.ArrayList.java;

public class ClassesSunway {
private String Type;
private int NoOfDesk;
private int NumberOfStudents;
public ClassesSunway(String type, int NoOfDesk, int numberOfStudents) {
	super();
	this.Type = type;
	this.NoOfDesk = NoOfDesk;
	this.NumberOfStudents = numberOfStudents;
}
public String getType() {
	return Type;
}
public void setType(String type) {
	Type = type;
}
public int getNoOfDesk() {
	return NoOfDesk;
}
public void setNoOfDesk(int noOfDesk) {
	NoOfDesk = noOfDesk;
}
public int getNumberOfStudents() {
	return NumberOfStudents;
}
public void setNumberOfStudents(int numberOfStudents) {
	NumberOfStudents = numberOfStudents;
}
public void run() {
	System.out.println("We are learning java here"); 
	
}



}

